{
    "enable": true,
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-empty-interface": false,
        "quotemark": false,
        "no-bitwise": false,
        "no-empty": false,
        "no-console": false,
        "no-string-literal": false,
        "object-literal-sort-keys": false,
        "interface-over-type-literal": false,
        "max-line-length": 140
    },
    "rulesDirectory": []
}